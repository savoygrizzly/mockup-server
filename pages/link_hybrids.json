{
  "pageName": "Hybrids",
  "pageDesc": "Discover our new Hybrids, a revolutionary combination for the best of both worlds.",
  "components": [
    {
      "type": "Header",
      "props": {
        "title": "Hybrids",
        "slogan": "A new era.",
        "image": "v2/pages/Hybrids/cover.jpg",
